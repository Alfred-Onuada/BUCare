    <% if (pages.therapists) { %>
      <section class="container py-xl-2 py-3 my-5">
        <% for (let i = 0; i < pages.therapists.length; i++) { %>
				<% let therapist = pages.therapists[i]; %>
          <div class="row eachTheraist">
            <div class="col-md-12">
              <div class="row">
                <div class=" col-md-3 ">
                  <img src="defaults/<%= therapist.First_Name.toLowerCase() %>.jpg" class="img-fluid" alt="A picture of <%= therapist.First_Name + " " + therapist.Last_Name %>">
                </div>
                <div class="col-md-9">
                  <h4 class="therapistBioName"><%= therapist.First_Name + " " + therapist.Last_Name %></h4>
                  <h4 class="therapistBioQualifications">Qualification: <%= therapist.Education_Level.join(", ") %></h4>
                  <h4 class="therapistBioSpecializations">Specialization: <%= therapist.Specialization.join(", ") %></h4>
                  <h4 class="therapistBioRating">Rating: 
                    <% if (therapist.Average_Rating != undefined) { %> 
                      <div <%= "style=width:"+ ((therapist.Average_Rating / 5) * 100) +"px;" %>>
                        <div>
                          <span class="fa fa-star"></span>
                          <span class="fa fa-star"></span>
                          <span class="fa fa-star"></span>
                          <span class="fa fa-star"></span>
                          <span class="fa fa-star"></span>
                        </div>
                      </div> 
                    <% } else { %>
                      Not available 
                    <% } %>
                  </h4>
                  <% for (let j = 0; j < therapist.Brief_Bio.length; j++) { %>
                    <p class="therapistBio">
                      <%= therapist.Brief_Bio[j] %>
                    </p>
                  <% } %>
                  <div class="buttons mt-3 move-right">
                    <% if (userStatus && userStatus.isClient) { %>
                      <!-- only logged in users are allowed to book therapists -->



                      <!-- figure out a way to show awaiting response here even after refresh if the person has booked before -->

                      <button id="joinRoomBtn-<%= therapist._id %>-fake" data-bs-toggle="modal" data-bs-target="#SelectProblemModal" data-specialization="<%= therapist.Specialization.join(", ") %>" onclick="selectProblem(this.dataset, 'joinRoomBtn-<%= therapist._id %>')" class="btn btn-style btn-primary" style="margin-right: 15px;">Book Now</button>
                      <button id="joinRoomBtn-<%= therapist._id %>" onclick="sendJoinRoomRequest('<%= therapist._id %>')" hidden></button>
                    <% } %>
                    <!-- <a href="" class="btn btn-white btn-primary">View Profile</a> -->
                  </div>
                  <hr>
                </div>
              </div>
            </div>
          </div>
        <% } %>
      </section>
    <% } %>

